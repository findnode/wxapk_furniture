.app{
  font-size: 32rpx !important;
  overflow-x: hidden;
}
.head{
  width: 100%;
  height: 7rpx;
  margin-top: 1rpx;
  border-top: 1rpx solid #eee;
}
.main{
  clear: both;
  background: #fafafa;
  padding: 20rpx 0;
  padding-left: 4%;
  /* border-top: 1rpx solid #f0f0f0; */
  float: left;
  width: 100%;
  margin-top: 10rpx;
}
.main>image{
  background: white;
  width: 200rpx;
  height: 200rpx;
  vertical-align: top;
  float: left;
}
.main>view{
  display: block;
  position: relative;
  left: 30rpx;
  top: 20rpx;
}
.name{
  font-size: 32rpx;
}
.gg{
  font-size: 30rpx;
  margin-top:25rpx;
}
.price{
  position: relative;
  top: 50rpx;
  font-size: 32rpx;
  color: red;
}
.num{
  position: absolute !important;
  right: 4%;
  font-size: 32rpx;
  text-align: right;
  margin-top: 10rpx;
}
.th,.thr,.thdh,.mjly,.hj,.je,.yf{
  float: left;
  width: 100%;
  font-size: 32rpx;
  margin-left: 4%;
  padding: 28rpx 0;
  border-bottom: 1rpx solid #eee;
}
.line{
  float: left;
  width: 100%;
  font-size: 32rpx;
  margin-left: 4%;
  padding-top: 28rpx;
}
.line2{
  float: left;
  width: 100%;
  font-size: 32rpx;
  /* margin-left: 4%; */
  padding-top: 28rpx;
  background: #fafafa; 
  /* border-bottom: 1rpx solid #f1f1f1; */
  border-bottom: 1rpx solid #fafafa;
}
.th>text,.thr>text,.thdh>text,.mjly>text,.hj>text,.je>text,.yf>text{
  font-size: 32rpx;
  width: 65%;
  float: right;
  /* padding-right: 4%; */
  text-align: right;
  position: relative;
  right: 8%;
}
.mp{
  float: right;
  margin-right: 12%;
}
.je>text,.yf>text,.name>text:nth-child(2){
  color: #b98152;
}
.yf>text,.je>text,.hj>text{
  font-size: 32rpx;
}
.je{
  margin-bottom: 100rpx;
}
/* .yf{
  margin-bottom: 100rpx;
} */
.foot{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  color: white;
  line-height: 90rpx;
  /* font-size:30rpx !important; */
}
.foot>view{
  text-align: center;
}
.fa{
  float: left;
  width: 50%;
  text-align: center;
  background: orange;
}
.fa1{
  float: left;
  width: 50%;
  text-align: center;
  background: rgba(255, 60, 60, 1);
}
.fb{
  background: #0D93BF;
}
.fb2{
  background: #ccc;
}
.fc{
  /* width: 50%; */
  width: 100%;
  display: inline-block;
  background: orange;
}
.fd{
  width: 50%;
  display: inline-block;
  background: #e3473d;
}